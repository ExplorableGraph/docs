<h3>
  {{if ./static, 'static ' }}
  {{if ./async, 'async '}}
  {{if ./generator, '*'}}{{./name}}()
  {{if ./constructor, 'constructor'}}
</h3>
<ul>
{{map ./parameters, =`
  <li>{{./name}}: {{ @template/scope/typeList.ori(./types) }}
    {{if ./description, =`
      &ndash; {{./description}}
    `}}
  </li>  
`}}
</ul>
{{unless ./constructor, =`
<p>
  Returns: {{ @template/scope/typeList.ori(./returnTypes) }}
</p>
`}}
<p>
  {{mdHtml ./description}}
</p>
