# netlify won't deploy a file whose name starts with a splat, preventing us from
# deploying .keys.json files. See the `netlify` command in the root folder.

/.keys.json /keys.json 200
/\*/.keys.json /:splat/keys.json 200

# Support redirects so SiteGraph works

/samples/cli/* /samples/cli/:splat
/samples/cli /samples/cli/
/samples/greetings/* /samples/greetings/:splat
/samples/greetings /samples/greetings/
/samples/help/* /samples/help/:splat
/samples/help /samples/help/
/samples/templates/* /samples/templates/:splat
/samples/templates /samples/templates/

/samples /samples/
