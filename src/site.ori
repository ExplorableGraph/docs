// The top-level structure of the site
{
  // Static files
  …client
  
  // All generated pages
  …data.ori/pages

  // This components.js "bundle" is just concatenated files
  components.js = @concat(components)

  // Live demos
  demos = {
    // Use `.` to avoid referencing the `demos` parent here
    aboutUs = ./demos/aboutUs/site.ori/
  }

  // Used by Netlify
  _redirects

  samples = samples.ori/
}
