{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "ori",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/node_modules/@explorablegraph/explorable/src/cli/cli.js",
      "cwd": "${workspaceFolder}/src/js/",
      "args": ["addPaths languages.yaml"]
    },
    {
      "name": "tsdoc",
      "program": "${workspaceFolder}/node_modules/@explorablegraph/explorable/src/cli/cli.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node",
      "cwd": "${workspaceFolder}/sandbox/tsdoc",
      "args": ["tsdoc greet.js"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "serve",
      // "runtimeExecutable": "node",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/node_modules/@explorablegraph/explorable/src/cli/cli.js",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--preserve-symlinks"],
      // "cwd": "${workspaceFolder}/site",
      // "args": ["serve watch virtual/src/site"]
      // "args": ["serve virtual/src/site"]
      "args": ["serve"]
    }
  ]
}
